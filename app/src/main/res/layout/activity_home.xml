<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/home_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app1="http://schemas.android.com/apk/res-auto">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="45dp"
        app1:layout_constraintBottom_toBottomOf="parent"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toTopOf="parent" />


    <com.tenclouds.fluidbottomnavigation.FluidBottomNavigation
        android:id="@+id/fluidBottomNavigation"
        android:layout_width="0dp"
        android:background="@drawable/shadow"
        android:layout_height="wrap_content"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintBottom_toBottomOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toTopOf="parent"
        app1:backColor="@color/colorAccent"
        app1:layout_constraintVertical_bias="1.0"
        />




